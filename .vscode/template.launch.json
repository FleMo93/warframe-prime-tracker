{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:8081/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "breakOnLoad": true,
      "breakOnLoadStrategy": "instrument",
      "sourceMapPathOverrides": {
        "webpack://PrimeTracker/*": "${webRoot}/*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Calculate data",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\calculateData.js"
    }
  ]
}